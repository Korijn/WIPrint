{
  "scripts": {
    "build": "npm run lint && npm run clean && npm run webpack && npm run package",
    "build:dev": "npm run lint && npm run clean && npm run webpack && npm run package:dev && npm run publish:dev",
    "package": "tfx extension create  --rev-version",
    "package:dev": "tfx extension create --overrides-file vss-extension.dev.json --rev-version",
    "publish": "tfx extension publish",
    "publish:dev": "tfx extension publish --overrides-file vss-extension.dev.json",
    "clean": "rimraf ./dist && rimraf ./*.vsix",
    "lint": "tslint -c tslint.json 'src/*.ts'",
    "test": "mocha",
    "webpack": "webpack --progress --colors --config webpack.config.js --output-path ./dist -p"
  },
  "devDependencies": {
    "amd-loader":"^0.0.8",
    "rimraf": "^2.6.1",
    "tfx-cli": "^0.4.5",
    "typescript": "2.3.4",
    "ts-loader": "^2.0.0",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "webpack": "^2.5.1"
  },
  "dependencies": {
    "@types/jquery": "^2.0.34",
    "@types/q": "0.0.32",
    "moment": "^2.18.1",
    "office-ui-fabric-js": "^1.4.0",
    "vss-web-extension-sdk": "1.109.0"
  },
  "name": "wiprint",
  "private": true,
  "version": "0.0.0"
}
